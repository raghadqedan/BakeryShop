 @model int


<a asp-action="Details" asp-route-id="@Model" class="btn btn-primary btn-sm ">
        <i class="fas fa-folder">
        </i>
        View
    </a>
<a asp-action="Edit" asp-route-id="@Model" class="btn btn-info btn-sm " >
        <i class="fas fa-pencil-alt">
        </i>
        Edit
    </a>
<a href="javascript:void(0)" data-id=@Model class="btn btn-danger btn-sm delete-btn">
        <i class="fas fa-trash">
        </i>
        Delete
    </a>
